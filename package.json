  },
  "homepage": "https://github.com/anticoders/gagarin",
  "keywords": [
    "gagarin",
    "meteor",
    "test",
    "mocha"
  ],
  "license": "MIT",
  "main": "gagarin.js",
  "name": "gagarin",
  "scripts": {
    "cli": "./node_modules/babel-cli/bin/babel-node.js --presets node6 ./cli.js",
    "coverage": "./node_modules/babel-cli/bin/babel-node.js --presets node6 ./coverage.js",
    "test": "./node_modules/@babel/node/bin/babel-node.js --presets @babel/preset-env ./test.js",
    "test:setup": "npm run test:example:install && npm run test:build_error:install",
    "test:example:install": "cd tests/example && meteor npm install",
    "test:build_error:install": "cd tests/build_error && meteor npm install"
  },
  "version": "1.0.0"
