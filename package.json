    "glob": "^7.1.2",
    "istanbul-lib-coverage": "^2.0.5",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "mocha": "^8.0.1",
    "mongodb": "^3.5.8",
    "portscanner": "^2.1.1",
    "right-pad": "^1.0.1",
    "underscore": "^1.9.0",
    "wd": "^1.12.1",
